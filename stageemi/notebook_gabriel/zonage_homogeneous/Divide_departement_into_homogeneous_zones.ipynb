    "df_zones=pd.DataFrame(columns=['zone_winning','zw_'+distance_choice,'zone_comp','zc_'+distance_choice,\n",
    "                                'zone_winning_2','zw2_'+distance_choice,'zone_winning_comp','zwc_'+distance_choice,\n",
